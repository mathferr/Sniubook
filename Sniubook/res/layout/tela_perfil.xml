<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.com.main.TelaPerfil" >

    <TextView
        android:id="@+id/tvNomeCursoAvaliacao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="RA:" />

    <TextView
        android:id="@+id/tvPrincipalCurso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tvNomeCursoAvaliacao"
        android:layout_below="@+id/tvNomeCursoAvaliacao"
        android:layout_marginTop="16dp"
        android:text="Nome:" />

    <TextView
        android:id="@+id/tvPrincipalCampus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tvPrincipalCurso"
        android:layout_below="@+id/tvPrincipalCurso"
        android:layout_marginTop="14dp"
        android:text="CPF:" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tvPrincipalCampus"
        android:layout_below="@+id/tvPrincipalCampus"
        android:layout_marginTop="14dp"
        android:text="Curso:" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="18dp"
        android:text="Periodo:" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="16dp"
        android:text="Campus:" />

    <TextView
        android:id="@+id/tvCampus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_toRightOf="@+id/textView6" />

    <TextView
        android:id="@+id/tvRegistroAcademico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tvNomeCursoAvaliacao"
        android:layout_alignLeft="@+id/tvCampus"
        android:layout_alignRight="@+id/tvCampus" />

    <TextView
        android:id="@+id/tvNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tvPrincipalCurso"
        android:layout_alignBottom="@+id/tvPrincipalCurso"
        android:layout_alignLeft="@+id/tvRegistroAcademico"
        android:layout_alignRight="@+id/tvRegistroAcademico" />

    <TextView
        android:id="@+id/tvCPF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView4"
        android:layout_alignLeft="@+id/tvNome"
        android:layout_alignRight="@+id/tvNome" />

    <TextView
        android:id="@+id/tvCurso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/tvCPF"
        android:layout_alignRight="@+id/tvCPF"
        android:layout_alignTop="@+id/textView4" />

    <TextView
        android:id="@+id/tvPeriodo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/tvCurso"
        android:layout_alignRight="@+id/tvCampus" />

    <Button
        android:id="@+id/btDeslogar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="109dp"
        android:text="Voltar" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="19dp"
        android:text="Turma:" />

    <TextView
        android:id="@+id/tvTurma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignLeft="@+id/tvCampus"
        android:layout_alignRight="@+id/tvCampus" />

</RelativeLayout>
